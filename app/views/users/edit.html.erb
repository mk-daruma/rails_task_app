<h3>スケジュール新規作成</h3>

<%= form_with model:@user do |f| %>
  <table>
    <tr>
      <th><%= f.label :title,"タイトル" %></th>
      <td><%= f.text_field :title %></td>
    </tr>
    <tr>
      <th><%= f.label :startDay,"開始日" %></th>
      <td><%= f.date_field :startDay %></td>
    </tr>
    <tr>
      <th><%= f.label :endDay,"終了日" %></th>
      <td><%= f.date_field :endDay %></td>
    </tr>
    <tr>
      <th><%= f.label :allDay,"終日" %></th>
      <td><%= f.check_box :allDay,{},"○"," " %></td>
    </tr>
    <tr>
      <th><%= f.label :scheduleMemo,"スケジュールメモ" %></th>
      <td><%= f.text_area :scheduleMemo %></td>
    </tr>
  </table>

  <div>
    <ul>
      <li><%= f.submit"スケジュールを編集を完了する" %></li>
      <li><%= link_to"スケジュール一覧に戻る",:users %></li>
    </ul>
  </div>
<% end %>